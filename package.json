{
  "name": "iuvox",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon server --verbose --ignore '*.mjs' ",
    "build": "npm run build:client && npm run build:server",
    "build:noExternal": "npm run build:client && npm run build:server:noExternal",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.js --outDir dist/server",
    "build:server:noExternal": "vite build --config vite.config.noexternal.js --ssr src/entry-server.js --outDir dist/server",
    "generate": "vite build --ssrManifest --outDir dist/static && npm run build:server",
    "serve": "cross-env NODE_ENV=production node server",
    "debug": "node --inspect-brk server",
    "prerender-dev": "node prerender-dev.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Iuvox/iuvox.git"
  },
  "dependencies": {
    "@vue/compiler-sfc": "^3.3.4",
    "@vue/server-renderer": "^3.3.4",
    "@vueuse/core": "^10.4.1",
    "@vueuse/head": "^1.3.1",
    "axios": "^1.5.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "lucide-vue-next": "^0.274.0",
    "pinia": "^2.1.6",
    "tsparticles-slim": "^2.12.0",
    "vue3-particles": "^2.12.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.3.4",
    "autoprefixer": "^10.4.15",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.29",
    "sass": "^1.66.1",
    "tailwindcss": "^3.3.3",
    "vite": "^4.4.9",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  }
}
